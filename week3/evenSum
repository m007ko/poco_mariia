//Write a function that takes an array of numbers and returns the sum of all even numbers.

let nums = [1, 2, 3, 7, 8, 9, 100]

function evenSum (nums){
    let Sum = 0;
    for (let num of nums){
        if (num%2 === 0){
            Sum +=num;
        } else {
            continue
        }
    }
    return Sum;
}

console.log(evenSum(nums))





