for (item of listLanguage {
//if (imput  )

switch (language) {
    case 'es': console.log("Hola Mundo"); break;
    case'de': console.log("Hallo die Welt"); break;
    case 'en': console.log("Hello World"); break;
    default: console.log("Hello World");
}
